<Window x:Class="DumpApplic.CheckTapes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DumpApplic"
        mc:Ignorable="d"
        Title="CheckTapes" Height="280" Width="450" MinHeight="280" MinWidth="450">
    <Viewbox VerticalAlignment="Stretch" Margin="10">
        <WrapPanel Name="MainWrapPanel" Orientation="Vertical" HorizontalAlignment="Center">
            <WrapPanel Name="TapesToCheck" Orientation="Vertical" HorizontalAlignment="Center">
                
            </WrapPanel>
            <WrapPanel Orientation="Horizontal" Margin="5" VerticalAlignment="Bottom" HorizontalAlignment="Center">
                <Button Name="CheckBtn" Height="30" Width="120" Margin="5" Content="Check" Click="CheckBtn_Click"/>
                <Button Name="CloseBtn" Height="30" Width="120" Margin="5" Content="Close" Click="CloseBtn_Click"/>
            </WrapPanel>
            </WrapPanel>
    </Viewbox>
</Window>
